                $s2 = Compute("$var**2+$bPlusC*$var+$bc")->reduce;
                $s3 = Compute("$a*($var**2+$bPlusC*$var+$bc)")->reduce;
                $s4 = Compute("$a*$var")->reduce;

                $problem = Compute("$a*$var*($var+$b)*($var+$c)")->reduce;
                $ans = Compute("$a*$var**3+$d*$var**2+$e*$var")->reduce;
            </pg-code>
