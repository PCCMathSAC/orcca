                <![CDATA[
                    \begin{tikzpicture}
                        \begin{axis}[xmin=-10,xmax=10,ymin=-10,ymax=10,
                            xtick={-5,0,5},minor xtick={-10,-9,...,10},
                            ytick={-5,0,5},minor ytick={-10,-9,...,10}]
                            \addplot+[domain=-1.123:7.123] {x^2-6*x+2};
                            \addplot[soliddot] coordinates {(0,2) (0.354,0) (5.646,0) (3,-7) (6,2)};
