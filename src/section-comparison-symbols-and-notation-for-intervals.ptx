            <pg-code>
              Context()->operators->redefine('>',using=>',',from=>'Numeric');
              Context()->lists->set('List' => {separator => '>'});

              @a = (-10..10)[NchooseK(21,5)];
              @a = map{Real($_)} (@a);
              $as = join('\quad', map{'{' . $_ . '}'} (@a));
              @b = PGsort(sub{$_[0] > $_[1]}, @a);
              $bs = join('>', @b);

              $comparison = Compute("$bs")->cmp(ordered => 1);
            </pg-code>
