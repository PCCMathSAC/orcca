            <mrow>\amp=\frac{\cancelhighlight{2}}{3\cdot\cancelhighlight{3}}\cdot\frac{\cancelhighlight{3}}{\cancelhighlight{2}\cdot5}</mrow>
            <mrow>\amp=\frac{1}{15}</mrow>
        </md>

