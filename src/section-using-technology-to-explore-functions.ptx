            <exercise>
                <webwork>
                    <setup>
                        <pg-code>
                            Context("Interval");
                            Context()->noreduce('(-x)-y','(-x)+y');
                            $m = random(1001,9999,1)*random(-1,1,2);
                            $b = random(1001,9999,1)*random(-1,1,2);
                            $func = Formula("$m x + $b")->reduce;
                            #important values to appear in plot
                            @x = (0,-$b/$m);
                            @y = (0,$b);
                            #one good answer
                            $xbound = 10**ceil(log(abs(-$b/$m))/log(10));
                            $ybound = 10**ceil(log(abs($b))/log(10));
                            $x = Interval("[-$xbound,$xbound]");
                            $y = Interval("[-$ybound,$ybound]");
                            $multians = MultiAnswer($x, $y)->with(
                              singleResult => 1,
                              checker => sub {
                                my ($correct, $student, $self) = @_;
                                my ($xstu, $ystu) = @{$student};
                                my ($xcor, $ycor) = @{$correct};
                                my ($xmin,$xmax) = $xstu->value;
                                my ($ymin,$ymax) = $ystu->value;
                                #Check important values are all present;
                                for my $xval (@x) {
                                  if (!$xstu->contains($xval)) {
                                    Value::Error('Your x-range is not large enough');
                                  }
                                }
                                for my $yval (@y) {
                                  if (!$ystu->contains($yval)) {
                                    Value::Error('Your y-range is not large enough');
                                  }
                                }
                                #Check important values are not right on the boundary;
                                my %xbounds = map { $_ => 1 } ($xmin,$xmax);
                                my %ybounds = map { $_ => 1 } ($ymin,$ymax);
                                for my $xval (@x) {
                                  if (exists($xbounds{$xval})) {
                                    Value::Error('An important x-value is right on the edge of your window');
                                  }
                                }
                                for my $yval (@y) {
                                  if (exists($ybounds{$yval})) {
                                    Value::Error('An important y-value is right on the edge of your window');
                                  }
                                }
                                #Check important x-values are not visually too close together;
                                #For a 160-pixel wide TI-89, we ask for at least four pixels of separation
