                  <mrow><var name="$rs[3]" />\amp=<var name="$y[3]" /></mrow>
                  <mrow><var name="$rs[4]" />\amp=<var name="$y[4]" /></mrow>
                  <mrow><var name="$rs[5]" />\amp=<var name="$y[5]" /></mrow>
                  <mrow><var name="$rs[6]" />\amp=<var name="$y[6]" /></mrow>
                </md>
              </p>

