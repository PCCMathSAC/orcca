      <exercise>
        <statement>
          <p>
            Create a table of ordered pairs and then make a plot of the equation <m>y=-\frac{3}{4}x+2</m>.
          </p>
        </statement>
        <answer>
          <sidebyside widths="47% 47%" margins="0%">

            <tabular valign="top" bottom="minor">
              <col halign="right" right="minor"/>
              <col halign="center" right="minor"/>
              <col halign="center"/>
              <row bottom="medium">
                <cell><m>x</m></cell>
                <cell halign="center"><m>y=-\frac{3}{4}x+2</m></cell>
                <cell>Point</cell>
              </row>
              <row>
                <cell><m>-4</m></cell>
                <cell><m>5</m></cell>
                <cell><m>(-4,5)</m></cell>
