                            \addplot[soliddot] coordinates {(3,0) (4,0) (0,12) (3.5,-0.25) (7,12)};
                        \end{axis}
                    \end{tikzpicture}
                ]]>
            </latex-image>
              </image>

            </figure>
          </sidebyside>
        </solution>
