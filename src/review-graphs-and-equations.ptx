    <exercisegroup cols="2">
      <exercise>
        <webwork>
          <setup>
            <pg-code>
              Context("Point");
              $intersections = List("(-6,5), (2,5)");
              Context("FiniteSolutionSets");
              Context()->flags->remove("NumberCheck");
              $solutions = Formula("{-6,2}");
              Context("Interval");
              $ineq[0] = Interval("(-inf,-6)U(2,inf)");
              $ineq[1] = Interval("[-6,2]");
              $gr = init_graph(-7,-7,7,7,
                axes=>[0,0],
                grid=>[14,14],
                size=>[240,140]
              );
              $gr->lb('reset');
              $gr->lb( new Label(1,0,'1','black','center','top','large'));
              $gr->lb( new Label(0,1,'1','black','left','middle','large'));
              Context()->flags->set(reduceConstants=>0);
              $f[0] = Formula("1/2 x^2+2x-1");
              $f[1] = Formula("5");
              @color = ('blue','red');
              @x = ([-6.47,2.47],[-6.9,6.9]);
              @px = (28/240,28/140);
              Context("Vector");
              $A[0] = Matrix("[[cos(pi/6),-sin(pi/6)],[sin(pi/6),cos(pi/6)]]");
              $A[1] = Matrix("[[cos(-pi/6),-sin(-pi/6)],[sin(-pi/6),cos(-pi/6)]]");
              for my $i (0,1) {  #function 1 and 2
                add_functions($gr, "$f[$i] for x in &lt;$x[$i][0],$x[$i][1]> using color:$color[$i] and weight:1");
                for my $j (0,1) {  #left and right
                  $fp[$i][$j] = Point($x[$i][$j],$f[$i]->eval(x=>$x[$i][$j]));
                  $df[$i][$j] = unit(Vector($px[0],$f[$i]->eval(x=>($x[$i][$j]+$px[0])) - $f[$i]->eval(x=>$x[$i][$j])));
                  for my $k (0,1) { #rotate left and right
                    $Adf[$i][$j][$k] = Vector($A[$k]*$df[$i][$j]);
                    $gr->moveTo($fp[$i][$j]->value);
                    $gr->lineTo(Point($fp[$i][$j]+(-1)**$j*unit($Adf[$i][$j][$k]))->value,$color[$i],1);
                  };
                };
              };
            </pg-code>
          </setup>
