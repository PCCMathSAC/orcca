    <exercisegroup permid="Ivx">
      <title>Challenge</title>
      <exercise permid="WNx">
<!-- Kara's challenge problem -->
        <webwork>
            <setup>

            <pg-code>
              Context("Point");
              Context()->variables->are(n=>'Real',p=>'Real');
              $xintanswer = Compute("((-n-sqrt(n^2-4p))/2,0),((-n+sqrt(n^2-4p))/2,0)");
              $yintanswer = Point(0,p);
              $vertexanswer = Compute("(-n/2,(4p-n^2)/4)");
            </pg-code>
            </setup>
            <statement>
              <p permid="AvM">
                Consider the function <m>f(x) = x^{2}+nx+p</m>.
                Let <m>n</m> and <m>p</m> be real numbers.
                Give your answers as points.

                <ol label="a." permid="UCL">
                  <li permid="yzi">
                    <p permid="hfy">
                      Suppose the function has two real <m>x</m>-intercepts.
                      What are they?
                    </p>
                    <instruction>
                      Enter the two intercepts as a comma-separated list. Be mindful of how you use parentheses in typing up the answers.
                    </instruction>
                    <p permid="NmH">
                      <var name="$xintanswer" width="30"/>
                    </p>
                  </li>

                  <li permid="eGr">
                    <p permid="ttQ">
                      What is its <m>y</m>-intercept?
                    </p>

                    <p permid="ZAZ">
                      <var name="$yintanswer" width="10"/>
                    </p>
                  </li>

                  <li permid="KNA">
                    <p permid="FIi">
                      What is its vertex?
                    </p>

                    <p permid="lPr">
                      <var name="$vertexanswer" width="20"/>
                    </p>
                  </li>
                </ol>
              </p>
            </statement>
            <solution permid="fbQ">
              <p permid="XEI">
                <ol label="a." permid="PSS">
                  <li permid="qUJ">
                    <p permid="RWA">
                      Set the function equal to zero and use the quadratic formula.
                    </p>
                  </li>

                  <li permid="XbS">
                    <p permid="ydJ">
                      Set <m>x</m> equal to zero and solve.
                    </p>
                  </li>

                  <li permid="Djb">
                    <p permid="ekS">
                      The x-coordinate of the vertex is equal to <m> \frac{-b}{2a} </m>.
                    </p>
                  </li>
                </ol>
              </p>
            </solution>
        </webwork>
      </exercise>

