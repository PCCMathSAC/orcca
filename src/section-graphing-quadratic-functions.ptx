                                    ytick={-5,0,...,35},minor ytick={-10,-9,...,40}]
                                    \addplot+[domain=-7.186:0.186] {3*x^2+21*x+36};
                                    \addplot[soliddot] coordinates {(-4,0) (-3,0) (0,36) (-3.5,-0.75) (-7,36)};
                                \end{axis}
                            \end{tikzpicture}
                        ]]>
                    </latex-image-code>
                    </image>
                </figure>
            </sidebyside>
            </answer>
        </exercise>        

