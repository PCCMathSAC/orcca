                $f3R = int($f3*100+0.5)/100;

                $alt="This picture shows the graph of a radical function $fname(x)=$func.";

                ($min[0], $max[0], $min[1], $max[1], $ticknum[0], $ticknum[1], $xticks_ref, $yticks_ref, $marksep[0], $marksep[1]) = NiceGraphParameters([5*$b,-5*$b],[0,3*$c]);

                @xticks = @$xticks_ref;
                @yticks = @$yticks_ref;

                $gr = init_graph($min[0],$min[1],$max[0],$max[1],
