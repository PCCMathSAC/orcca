                        </var>
                        <var name="$f">
                            <static>\frac{812}{100}</static>
                        </var>
                        <pg-code>
                            Context("Fraction-NoDecimals");
                            $d=random(0.1,9.9,0.1)+random(1,9,1)/100;
                            $f=Fraction($d);
                        </pg-code>
                    </setup>
                    <statement>
                        <p>Write the rational number <m><var name="$d" /></m> as a fraction.</p>
                        <p><var name="$f" width="10" /></p>
