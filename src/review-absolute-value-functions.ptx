      <exercise permid="Bek">
 <!-- quadratic -->
        <webwork>
            <setup>

            <pg-code>
                $h = random(-4,4,1);
                $k = random(-4,4,1);
                $x = ($h &lt; 0) ? random(-6,$h-1,1) : random($h+1,6,1);
                $y = ($k &lt; 0) ? random($k+1,6,1) : random(-6,$k-1,1);
                $c = ($y-$k)/($x - $h)**2;
                $f = Formula("$c*(x-$h)^2+$k");

                $gr = init_graph(-7,-7,7,7,
                   axes=>[0,0],
                   grid=>[14,14],
                   size=>[240,240]
                );
                $gr->lb('reset');
                for $j (-6..6){
                    if (Real($j)==0) {next;}
                    $gr->lb(new Label($j,0,$j,'black','center','top','large'));
                    $gr->lb(new Label(0,$j,$j,'black','right','middle','large'));
                };
                $gr->lb(new Label(7,0,'x','black','right','bottom','large'));
                $gr->lb(new Label(0,7,'y','black','left','top','large'));
                $yexit = ($c &lt; 0) ? -7 : 7;
                @xvalues = num_sort(-7,7,sqrt(($yexit-$k)/$c)+$h,-sqrt(($yexit-$k)/$c)+$h);
                add_functions($gr, "$f for x in &lt;$xvalues[1],$xvalues[2]> using color:blue and weight:1");
                $px = 14/240;

                $gr->moveTo($xvalues[1]+$px,$c*($xvalues[1]+$px-$h)**2+$k);
                $gr->arrowTo($xvalues[1],$c*($xvalues[1]-$h)**2+$k,'blue',1);
                $gr->moveTo($xvalues[2]-$px,$c*($xvalues[2]-$px-$h)**2+$k);
                $gr->arrowTo($xvalues[2],$c*($xvalues[2]-$h)**2+$k,'blue',1);

                @x = num_sort($x,2*$h-$x);
                $ans[0] = ($c > 0) ? Union("(-inf,$x[0])U($x[1],inf)") : Interval("($x[0],$x[1])");
                $ans[1] = ($c > 0) ? Interval("[$x[0],$x[1]]") : Union("(-inf,$x[0]]U[$x[1],inf)");
            </pg-code>
            </setup>
            <statement>
              <p permid="Yux">
                A graph of <m>f</m> is given.
                Use the graph alone to solve the compound inequalities.
              </p>

              <sidebyside widths="40%" permid="lls">
                <image pg-name="$gr" />
              </sidebyside>
              <instruction>
                Write your solution sets in interval notation.
              </instruction>
              <p permid="EBG">
                <ol label="a." permid="cgb">
                  <li permid="Twj">
                    <p permid="nYU">
                      <m>f(x)\gt <var name="$y" /></m>
                    </p>

                    <p permid="Ugd">
                      <var name="$ans[0]" width="10" />
                    </p>
                  </li>

                  <li permid="zDs">
                    <p permid="Anm">
                      <m>f(x)\leq <var name="$y" /></m>
                    </p>

                    <p permid="guv">
                      <var name="$ans[1]" width="10" />
                    </p>
                  </li>
                </ol>
              </p>
            </statement>
        </webwork>
      </exercise>

