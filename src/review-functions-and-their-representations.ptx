      <exercise permid="Tbx">
        <webwork>
            <setup>

            <pg-code>
                Context("Interval");
                Context()->noreduce('(-x)-y','(-x)+y');
                $m = random(1001,9999,1)*random(-1,1,2);
                $b = random(1001,9999,1)*random(-1,1,2);
                $func = Formula("$m x + $b")->reduce;
                #important values to appear in plot
                @x = (0,-$b/$m);
                @y = (0,$b);
                #one good answer
                $xbound = 10**ceil(log(abs(-$b/$m))/log(10));
                $ybound = 10**ceil(log(abs($b))/log(10));
                $x = Interval("[-$xbound,$xbound]");
                $y = Interval("[-$ybound,$ybound]");
                $multians = MultiAnswer($x, $y)->with(
