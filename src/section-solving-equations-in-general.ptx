            Context("FiniteSolutionSets");
            $solution = ($nonsquarepart != 1) ?
              Formula("{$rational + $effcoeff sqrt($nonsquarepart),$rational - $effcoeff sqrt($nonsquarepart)}") :
              Formula("{$sum,$dif}");
          </pg-code>
          <statement>
            <p>
              Solve the equation.
            </p>
            <p>
              <m><var name="$left"/>=<var name="$right"/></m>
            </p>
            <p>
              <var name="$solution" width="60"/>
            </p>
          </statement>
        </webwork>
      </exercise>
