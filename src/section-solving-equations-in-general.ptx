            $dif = $rational - $coefficient;
            Context("FiniteSolutionSets");
            $solution = ($nonsquarepart != 1) ?
              Formula("{$rational + $effcoeff sqrt($nonsquarepart),$rational - $effcoeff sqrt($nonsquarepart)}") :
              Formula("{$sum,$dif}");
