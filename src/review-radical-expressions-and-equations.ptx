            <p>
              <md>
                <mrow><var name="$expression"/>\amp = \frac{\sqrt[<var name="$n"/>]{<var name="$y"/>}}{\sqrt[<var name="$n"/>]{<var name="$x"/>}}</mrow>
                <mrow>\amp = \frac{\sqrt[<var name="$n"/>]{<var name="$y"/>}}{<var name="$r"/>}</mrow>
