      <!-- root(n,x) with positive fraction x, denominator is nth power -->
      <exercise>
        <webwork>
          <pg-macros>
            <macro-file>contextForm.pl</macro-file>
          </pg-macros>
            <pg-code>
              Context("Form");
              do {
                $n = random(3,6,1);
                $r = random(2,5,1);
                $x = $r**($n);
                do {$y = ($n % 2 == 0) ? list_random(2,3,5..7,10,11) : list_random(2..7,9..12);} until (gcd($x,$y) == 1);
              } until ($x &lt;= 125);
              $expression = Formula("root($n,$y/$x)");
              $answer = Formula("1/$r root($n,$y)");
            </pg-code>
          <statement>
            <p>
              Simplify <m><var name="$expression"/></m>.
            </p>
            <instruction>To enter <m>\sqrt[n]{x}</m>, type <c>root(n, x)</c>. If the value is not a real number, enter <c>not a real number</c>.</instruction>
            <p>
              <var name="$answer" width="30"/>
