    <exercisegroup permid="mFc">
      <title>Challenge</title>
      <exercise permid="cus">
<!-- Kara's challenge problem -->
        <webwork>
            <setup>

             <pg-code>
               $c = list_random(2,4,5,7,8,10);
               $threec = $c*3;
               @smalls = ();
               @bigs = ();
               for $i (1..floor(sqrt($threec))) {
                 if ($threec % $i == 0) {
                   push(@smalls,$i);
                   push(@smalls,-$i);
                   push(@bigs,$threec/$i);
                   push(@bigs,-$threec/$i);
                 }
               }
               @b = ();
               $n = scalar(@smalls);
               for $j (1..$n) {
                 push(@b,$smalls[$j-1] + $bigs[$j-1]);
               }
               $answer = List(@b);

               Context()->variables->add(b=>'Real');
               $expression = Formula("3x^2+bx+$c")->reduce;
            </pg-code>
            </setup>
            <statement>
              <p permid="fDt">
                What integers can go in the place of <m>b</m> so that the quadratic expression <m><var name="$expression" /></m> is factorable?
              </p>
              <instruction>
                Enter the solutions as a comma-separated list.
              </instruction>
              <p permid="LKC">
                <var name="$answer" width="30"/>
              </p>
            </statement>
            <hint permid="FGV">
              <p permid="WjH">
                There are <var name="$n"/> answers.
              </p>
            </hint>
            <solution permid="ZzM">
              <p permid="qcy">
                Recall the <q>AC</q> method.
                When you multiply <m>3</m> by <var name="$c"/>, you get <var name="$threec"/>.
                Think of all of the ways to factor <var name="$threec"/>.
                The possible <m>b</m>-values are <var name="$answer"/>.
              </p>
            </solution>
        </webwork>
      </exercise>

    </exercisegroup>
