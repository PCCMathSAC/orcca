        <exercisegroup cols="2">
            <title>Identifying Linear Equations and Inequalities</title>
            <exercise>
                <webwork>
                    <setup>
                        <pg-code>
                            Context("Numeric");
                            Context()->variables->are(x=>'Real',y=>'Real',z=>'Real',r=>'Real',q=>'Real',p=>'Real',V=>'Real',);
                            Context()->noreduce('(-x)-y');
                            $yes=PopUp(['?','is','is not'],1);
                            $no=PopUp(['?','is','is not'],2);
                            @allvars=('x','y','z','r','q','p','V');
                            @var=(
                                list_random(@allvars),
                                (@allvars)[NchooseK(7,2)],
                                list_random(@allvars),
                                list_random(@allvars),
                                list_random(@allvars),
                                'r',
                            );
                            @left=(
                                Formula(non_zero_random(-9,9,1).$var[0].'+'.non_zero_random(-19,19,1))->reduce,
                                Formula(non_zero_random(-9,9,1).$var[1].'^2+'.non_zero_random(-9,9,1).$var[2])->reduce,
                                Formula(non_zero_random(-9,9,1).'+'.non_zero_random(-9,9,1).$var[3].'^2')->reduce,
                                Formula(non_zero_random(-9,9,0.01).$var[4])->reduce,
                                Formula('sqrt('.non_zero_random(-9,9,0.1).$var[5].'+'.non_zero_random(-9,9,1).')')->reduce,
                                Formula("2 pi r")->reduce
                            );
                            @right=(
                                Compute(random(-9,9,1)),
                                Compute(random(-29,29,1)),
                                Compute(random(-29,29,1)),
                                Compute(random(-9,9,1)),
                                Compute(random(0,9,1)),
                                Formula(random(1,20,1).'pi')->reduce
                            );
                            @eq = map{$left[$_]->TeX.'='.$right[$_]->TeX}(0..5);
                            @answer = (
                                $yes,
                                $no,
                                $no,
                                $yes,
                                $no,
                                $yes
                            );
                            @sol = (
                                "The equation is a linear equation in one variable.",
                                "The equation is not a linear equation in one variable since it contains two variables.",
                                "The equation is not a linear equation in one variable since it contains the square of a variable.",
                                "The equation is a linear equation in one variable.",
                                "The equation is not a linear equation in one variable since there is a variable within a square root.",
                                "The equation is a linear equation in one variable.",
                            );
                            @mix = NchooseK(6,6);
                            @mixeq = (@eq)[@mix];
                            @mixanswer = (@answer)[@mix];
                            @mixsol = (@sol)[@mix];
                        </pg-code>
                    </setup>
                    <statement>
                        <p>Are the equations below linear equations in one variable?<ol label="a.">
