                                Formula(non_zero_random(-9,9,0.1).$var[0])->reduce,
                                Formula(random(-200,200,1))->reduce,
                                Formula('sqrt('.random(2,9,1).$var[3].')+'.non_zero_random(-19,19,1))->reduce,
                            );
                            @right=(
                                Compute(random(-99,99,1)),
                                Formula(non_zero_random(-9999,9999,1).$var[1].'+'.non_zero_random(-9999,9999,1).$var[2])->reduce,
                                Compute(non_zero_random(-9,9,1)),
                            );
                            @eq = map{$left[$_]->TeX.list_random('\lt','\gt','\leq','\geq').$right[$_]->TeX}(0..2);
                            @answer = (
                                $yes,
                                $yes,
                                $no,
                            );
                            @sol = (
                                "The inequality is a linear inequality in one variable.",
                                "The inequality is a linear inequality in one variable.",
                                "The inequality is not a linear inequality in one variable since there is a variable inside a square root.",
                            );
                            @mix = NchooseK(3,3);
                            @mixeq = (@eq)[@mix];
                            @mixanswer = (@answer)[@mix];
                            @mixsol = (@sol)[@mix];
                        </pg-code>
                    </setup>
                    <statement>
                        <p>Are the inequalities below linear inequalities in one variable?<ol label="a.">
                            <li>
                                <p><m><var name="$mixeq[0]" /></m> <var name="$mixanswer[0]" form="popup" /> a linear inequality in one variable.</p>
                            </li>
                            <li>
                                <p><m><var name="$mixeq[1]" /></m> <var name="$mixanswer[1]" form="popup" /> a linear inequality in one variable.</p>
                            </li>
                            <li>
                                <p><m><var name="$mixeq[2]" /></m> <var name="$mixanswer[2]" form="popup" /> a linear inequality in one variable.</p>
                            </li>
                        </ol></p>
                    </statement>
                    <solution>
                        <p><ol label="a.">
                            <li>
                                <p><var name="$mixsol[0]" /></p>
                            </li>
                            <li>
                                <p><var name="$mixsol[1]" /></p>
                            </li>
                            <li>
                                <p><var name="$mixsol[2]" /></p>
                            </li>
                        </ol></p>
                    </solution>
                </webwork>
            </exercise>
