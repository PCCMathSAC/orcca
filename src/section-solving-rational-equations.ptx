      <exercise permid="KNJ">
        <webwork>
            <setup>

            <pg-code>
                Context("FiniteSolutionSets");
                Context()->flags->remove("NumberCheck");
                $solution = List(Formula("3.89506117471740"));
            </pg-code>
            </setup>
            <statement>
              <p permid="soG">
                Use technology to solve the equation
                <me permid="Wld">
                  \frac{10}{x^2+3}=\frac{x+1}{x+5}
                </me>.
              </p>
              <instruction>
                Round answers to at least four significant digits.
              </instruction>
              <p permid="YvP">
                <var name="$solution" width="20" />
              </p>
            </statement>
            <solution permid="LxD">
              <p permid="RWJ">
