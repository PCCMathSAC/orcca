        <exercisegroup cols="2">
            <introduction>
                <p>Use technology to find features of a quadratic function and its graph.</p>
            </introduction>
            <exercise>
                <webwork>
                    <pg-macros>
                        <macro-file>contextFiniteSolutionSets.pl</macro-file>
                    </pg-macros>
                    <setup>
                        <pg-code>
                            $f = RandomVariableName(type=>'function');
                            $bound = 10;
                            do {
                                $a = non_zero_random(-2,2,1);
                                $b = non_zero_random(-4,4,1);
                                $c = random(-5,5,1);
                                Context()->noreduce('(-x)+y', '(-x)-y');
                                $formula = Formula("$a x^2 + $b x + $c")->reduce;
                                $discriminant = Real("($b)^2-4*$a*$c");

                                Context("Point");
                                Context()->flags->set(tolerance => 0.001);
                                $h = -$b/(2*$a);
                                $k = $formula->eval(x=>$h);
                                $vertex = Point($h,$k);
                                $yint = Point(0,$c);
                                if ($discriminant == 0) {$xint = List(Point($h,0));}
                                elsif ($discriminant > 0) {$xint = List(Point($h-sqrt($discriminant)/(2*$a),0), Point($h+sqrt($discriminant)/(2*$a),0));}
                                else {$xint = List(Compute("NONE"));}

                                Context("Numeric");
                                Context()->flags->set(tolerance => 0.001);
                                $p = non_zero_random(-8,8,1);
                                $fofp = $formula->eval(x=>$p);
                                $q = ($a > 0) ? non_zero_random(ceil($k+1),ceil($k+10),1) : non_zero_random(floor($k-10),floor($k-1),1);

                                Context("FiniteSolutionSets");
                                Context()->flags->remove("NumberCheck");
                                Context()->flags->set(tolerance => 0.001);
                                $s1 = (-$b - sqrt(($b)**2 - 4*$a*($c-$q)))/(2*$a);
                                $s2 = (-$b + sqrt(($b)**2 - 4*$a*($c-$q)))/(2*$a);
                                ($s1, $s2) = (min($s1,$s2), max($s1,$s2));
                                $solutions = Formula("{$s1,$s2}");

                                Context("Interval");
                                Context()->flags->set(tolerance => 0.001);
                                $domain = Compute("(-inf,inf)");
                                $range = ($a > 0) ? Interval("[$k,inf)") : Interval("(-inf,$k]");
                                $inequality = list_random('&lt;', '>', '\leq', '\geq');
                                if    ($a > 0 and $inequality eq '&lt;' or $a &lt; 0 and $inequality eq '>')    {$ineqsolution = Interval("($s1,$s2)");}
                                elsif ($a > 0 and $inequality eq '>'    or $a &lt; 0 and $inequality eq '&lt;') {$ineqsolution = Union("(-inf,$s1)U($s2,inf)");}
                                elsif ($a > 0 and $inequality eq '\leq' or $a &lt; 0 and $inequality eq '\geq') {$ineqsolution = Interval("[$s1,$s2]");}
                                elsif ($a > 0 and $inequality eq '\geq' or $a &lt; 0 and $inequality eq '\leq') {$ineqsolution = Union("(-inf,$s1]U[$s2,inf)");}
                            } until (abs($h) &lt; $bound and abs($k) &lt; $bound and abs($c) &lt; $bound and abs($p) &lt; $bound and abs($fofp) &lt; $bound and abs($q) &lt; $bound and abs($s1) &lt; $bound and abs($s2) &lt; $bound and $fofp != 0);
                        </pg-code>
                    </setup>
                    <statement>
                        <p>Let <m><var name="$f" />(x)=<var name="$formula"/></m>. Use technology to find the following.</p>
                        <instruction>Any decimal answers should be correct to four significant digits.</instruction>
                        <p><ol label="a.">
                            <li>
                                <p>The vertex is <var name="$vertex" width="20" />.</p>
                            </li>
                            <li>
                                <p>The <m>y</m>-intercept is <var name="$yint" width="20" />.</p>
                            </li>
                            <li>
                                <p>The <m>x</m>-intercept(s) is/are <var name="$xint" width="20" />.</p>
                                <instruction>If there are no <m>x</m>-intercepts, enter <c>NONE</c>. If there are more than one, use commas to separate the points.</instruction>
                            </li>
                            <li>
                                <p>The domain of <m><var name="$f" /></m> is <var name="$domain" width="20" />.</p>
                                <instruction>Use interval notation.</instruction>
                            </li>
                            <li>
                                <p>The range of <m><var name="$f" /></m> is <var name="$range" width="20" />.</p>
                                <instruction>Use interval notation.</instruction>
                            </li>
                            <li>
                                <p>Calculate <m><var name="$f" />(<var name="$p" />)</m>. <var name="$fofp" width="20" />.</p>
                            </li>
                            <li>
                                <p>Solve <m><var name="$f" />(x)=<var name="$q" /></m>. <var name="$solutions" width="20" /></p>
                            </li>
                            <li>
                                <p>Solve <m><var name="$f" />(x)<var name="$inequality" /><var name="$q" /></m>. <var name="$ineqsolution" width="20" /></p>
                                <instruction>Use interval notation. If you need to write a union of intervals, use <c>U</c> for the union symbol.</instruction>
                            </li>
                        </ol></p>
                    </statement>
                </webwork>
            </exercise>
            <exercise>
                <webwork>
                    <pg-macros>
                        <macro-file>contextFiniteSolutionSets.pl</macro-file>
                    </pg-macros>
                    <setup>
                        <pg-code>
                            $f = RandomVariableName(type=>'function');
                            $bound = 10;
                            do {
                                $a = non_zero_random(-2,2,1);
