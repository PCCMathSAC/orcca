            </li>

            <li permid="FPM">
              <p permid="zxX">
                Vertex <m>(-3,-1)</m>, <m>a=2</m>
              </p>
            </li>

            <li permid="lWV">
              <p permid="fFg">
                Vertex <m>(5,12)</m>, <m>a=-3</m>
              </p>
            </li>
          </ol>
        </p>
      </statement>
