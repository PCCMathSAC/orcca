    <exercisegroup permid="Rep">
      <title>Challenge</title>
      <exercise permid="nKI">
<!-- Kara's challenge problem -->
        <webwork>
            <setup>

             <pg-code>
               $c = list_random(4,6,8,9,10,12,14,15,16,18,20)*random(-1,1,2);
               $absc = abs($c);
               @smalls = ();
               @bigs = ();
               for $i (1..floor(sqrt($absc))) {
                 if ($absc % $i == 0) {
                   push(@smalls,$i);
                   push(@smalls,-$i);
                   push(@bigs,$c/$i);
                   push(@bigs,-$c/$i);
                 }
               }
               @b = ();
               $n = scalar(@smalls);
               for $j (1..$n) {
                 push(@b,$smalls[$j-1] + $bigs[$j-1]);
               }
               $answer = List(@b);

               Context()->variables->add(b=>'Real');
               $expression = Formula("x^2+bx+$c")->reduce;
            </pg-code>
            </setup>
            <statement>
              <p permid="uyj">
                What integers can go in the place of <m>b</m> so that the quadratic expression <m><var name="$expression" /></m> is factorable?
              </p>
              <instruction>
                Enter the solutions as a comma-separated list.
              </instruction>
              <p permid="aFs">
                <var name="$answer" width="30"/>
              </p>
            </statement>
            <hint permid="BOn">
              <p permid="cQe">
                There are <var name="$n"/> answers.
              </p>
            </hint>
            <solution permid="VHe">
              <p permid="wIV">
                Think about all of ways to factor <var name="$c"/>.
                The list of possible <m>b</m>-values is <var name="$answer"/>.
              </p>
            </solution>
        </webwork>
      </exercise>

    </exercisegroup>
