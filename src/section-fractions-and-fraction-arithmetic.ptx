      <title>Review and Warmup</title>
      <exercise permid="vid">
        <webwork>
            <setup>

            <pg-code>
                Context("Fraction");
                $a=Fraction(random(-33,-13,4),4);
                $xmin = -max(int(Real($a)*1.6), 10);
                $gr = init_graph($xmin,-1,-$xmin,1,
                    axes=>[100,0],
                    size=>[240,34]
                );
                @ticks;
                for my $j (($xmin)..(-$xmin)) {push(@ticks,$j);};
                $gr->h_ticks(0,"black",@ticks);
                $gr->lb( new Label(0,-0.1,0,'black','center','top','large'));
                $gr->lb( new Label(1,-0.1,1,'black','center','top','large'));
                $j=0;
                @decoys = (Real($a), int(Real($a)/2), Real(-$a-2), -Real($a), );
                for $i (@decoys) {
                    $gr->moveTo($i,-1);$gr->lineTo($i,0,'black',1);
                    $gr->lb( new Label($i,0.05,$ALPHABET[$j],'black','center','bottom','large'));
                    $j++;
                }
                $popup=PopUp(['?', 'A', 'B', 'C', 'D'],'A');
            </pg-code>
            </setup>
            <statement>
              <p permid="ZlR">
