                                        minor xtick={0,500,...,8500}, minor ytick={0,500,...,8500},
                                        xtick={0,2000,...,8000},ytick={0,2000,...,8000}
                                    ]
                                        \addplot+[
                                            domain = 0:8000,
                                            -,
