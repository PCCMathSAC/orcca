    <exercisegroup cols="2" permid="kYX">
            <!-- Graphing with technology -->
      <exercise permid="RQa">
        <webwork>
            <pg-macros><macro-file>PCCgraphMacros.pl</macro-file></pg-macros>
            <setup>

            <pg-code>
                Context("Numeric");
                Context()->flags->set(showExtraParens=>0, reduceConstants=>0);
                Context()->noreduce("(-x)-y","(-x)+y");

                $fname = RandomVariableName(type=>function);
                $a = non_zero_random(-5,5,1);
                $b = non_zero_random(-5,5,1);

                $func = Formula("sqrt($a-x)+$b")->reduce;

                $alt="This picture shows the graph of a radical function $fname(x)=$func. The point ($a,$b) is labeled.";

                ($min[0], $max[0], $min[1], $max[1], $ticknum[0], $ticknum[1], $xticks_ref, $yticks_ref, $marksep[0], $marksep[1]) = NiceGraphParameters([0,1.5*$a],[0,2*$b]);

                @xticks = @$xticks_ref;
                @yticks = @$yticks_ref;

                $gr = init_graph($min[0],$min[1],$max[0],$max[1],
                   axes=>[0,0],
                   grid=>[$ticknum[0],$ticknum[1]],
                   size=>[xPixels(),yPixels()]
                );
                $gr->lb('reset');
                for my $j (@xticks) {
                    if (abs($j) &lt; 10**(-10)) {next;}
                    $gr->lb( new Label($j, -$marksep[1]/8, $j,'black','center','top'));
                }
                $gr->lb( new Label($max[0]-$marksep[0]/2, $marksep[1]/8, 'x','black','center','bottom'));
                $gr->lb( new Label($marksep[0]/8, $max[1]-$marksep[1]/2, 'y','black','left','middle'));

                for my $j (@yticks) {
                    if (Real($j) == 0) {next;}
                    $gr->lb( new Label(-$marksep[0]/8, $j, $j,'black','right','middle'));
                }

                $xfunc = sub { my $t = shift();
                    return $a-($t)**2; };
                $yfunc = sub { my $t = shift();
                    return $t+$b; };
                $funct = new Fun( $xfunc, $yfunc, $gr);
                $funct -> domain(0,10);

                $hx = ($a+$min[0])/2;
                $hy = $func->eval(x=>$hx);
                $gr->lb( new Label($hx, $hy+$marksep[1]/4, "$fname(x)",'blue','center','middle'));

                $gr->stamps( closed_circle($a, $b, 'red') );
                $gr->lb( new Label($a, $b-$marksep[1]/8, "($a,$b)",'red','center','top'));

                Context("Interval");
                $range = Compute("[$b,inf)");
            </pg-code>
            </setup>
            <statement>
              <p permid="saT">
                Use technology to find the range of the function <m><var name="$fname" /></m> defined by <m><var name="$fname" />(x)=<var name="$func" /></m>.
              </p>
              <instruction>
                If you need to enter <m>\infty</m>, you can type  <c>inf</c> or <c>infinity</c>.  If you need to union two (or more) intervals, use the letter <c>U</c>. For example: <c>(-inf,5) \cup (5, inf)</c>.
              </instruction>
              <p permid="Yic">
                <var name="$range" width="10" />
              </p>
            </statement>
            <solution permid="JQc">
              <p permid="Znv">
                We use technology to graph the function <m><var name="$fname" />(x)=<var name="$func" /></m>:
              </p>

              <sidebyside permid="cpf">

                <image pg-name="$gr" permid="Uid">
                  <description><var name="$alt" /></description>
                </image>

              </sidebyside>

              <p permid="FuE">
                The function's range is <m><var name="$range"/></m>.
              </p>
            </solution>
        </webwork>
      </exercise>

