                                            \addplot[soliddot,color=firstcolor] coordinates {(6,4)};
                                            \addplot[secondarycurve,domain=-6.5:0.5] {2*(x+1)+4} node[pos=0.5,sloped,above] {$y=2(x+1)+4$};
                                            \addplot[soliddot,color=secondcolor] coordinates {(-1,4)};
                                            \addplot[soliddot,color=thirdcolor] coordinates {(-6,-6)};
