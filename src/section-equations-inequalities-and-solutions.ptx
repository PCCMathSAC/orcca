                <row>
                  <cell><var name="$lefteval" width="3"/></cell>
                  <cell><m>\wonder{=}</m></cell>
                  <cell><m><var name="$righteval->TeX"/></m></cell>
                </row>
              </tabular>
              <p>
                <var name="$dropdown" form="popup"/>
              </p>
            </statement>
          </webwork>
