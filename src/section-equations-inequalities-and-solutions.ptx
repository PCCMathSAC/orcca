              <tabular>
                <col halign="right" />
                <col halign="center" />
                <col halign="left" />
                <row>
                  <cell><m><var name="$left->TeX"/></m></cell>
                  <cell><m>=</m></cell>
                  <cell><m><var name="$right->TeX"/></m></cell>
