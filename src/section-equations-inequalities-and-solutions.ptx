        <exercise label="check-solution-linear-equation-integer">
          <webwork xml:id="webwork-check-solution-linear-equation-integer">
            <pg-macros>
              <macro-file>parserPopUp.pl</macro-file>
            </pg-macros>
            <pg-code>
              $actual_solution = Real(non_zero_random(-9,9));
              $m = random(2,9);
              $b = non_zero_random(-9,9);
              $left = Formula("$m x + $b")->reduce;
              $right = Formula($left->eval(x => $actual_solution))->reduce;
              $proposed_solution = random(0,1) ? $actual_solution : $actual_solution + non_zero_random(-3,3);
              Context("LimitedNumeric");
              $lefteval = Real($left->eval(x => $proposed_solution));
              $righteval = Real($right->eval(x => $proposed_solution));
              $dropdown = DropDown(["Yes, it is a solution.", "No, it is not a solution."], ($lefteval == $righteval) ? 0 : 1, showInStatic => 0);
            </pg-code>
            <statement>
              <p>
                Is <m><var name="$proposed_solution"/></m> a solution to:
