                    <setup>
                        <pg-code>
                            $f = RandomVariableName(type=>'function');
                            $a = 1;
                            $h = non_zero_random(-9,9,1);
                            $k = non_zero_random(-9,9,1);
                            Context()->noreduce('(-x)+y','(-x)-y');
                            $formula = Formula("$a(x - $h)^2 + $k")->reduce;
                            Context("Interval");
                            $domain = Compute("(-inf,inf)");
                            $range = Compute("[$k,inf)");
                            Context("Point");
                            $vertex = Compute("($h,$k)");
                            $posneg = ($a > 0) ? "positive" : "negative";
